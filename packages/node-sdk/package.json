{
  "name": "@airaccount/sdk",
  "version": "0.1.0-alpha.1",
  "description": "AirAccount Node.js SDK for Web3 applications with TEE hardware wallet integration",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.esm.js",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "files": [
    "dist",
    "wasm",
    "README.md"
  ],
  "scripts": {
    "build": "tsup src/index.ts --format cjs,esm --dts --clean",
    "build:wasm": "wasm-pack build --target bundler --out-dir ../node-sdk/wasm ../core-logic",
    "dev": "tsup src/index.ts --format cjs,esm --dts --watch",
    "test": "jest",
    "test:watch": "jest --watch",
    "lint": "eslint src --ext .ts,.tsx",
    "lint:fix": "eslint src --ext .ts,.tsx --fix",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "npm run build",
    "docs": "typedoc src/index.ts",
    "example:basic": "node examples/basic.js",
    "example:web": "node examples/web-integration.js"
  },
  "keywords": [
    "airaccount",
    "web3",
    "wallet",
    "tee",
    "hardware-wallet",
    "rust",
    "wasm",
    "blockchain",
    "ethereum",
    "bitcoin",
    "security"
  ],
  "author": "AirAccount Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/aastar/AirAccount.git",
    "directory": "packages/node-sdk"
  },
  "bugs": {
    "url": "https://github.com/aastar/AirAccount/issues"
  },
  "homepage": "https://airaccount.io",
  "dependencies": {
    "eventemitter3": "^5.0.1",
    "ws": "^8.16.0",
    "node-fetch": "^3.3.2",
    "viem": "^2.7.0",
    "ethers": "^6.10.0",
    "@simplewebauthn/browser": "^9.0.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.11",
    "@types/node": "^20.10.6",
    "@types/ws": "^8.5.10",
    "@typescript-eslint/eslint-plugin": "^6.17.0",
    "@typescript-eslint/parser": "^6.17.0",
    "eslint": "^8.56.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "tsup": "^8.0.1",
    "typedoc": "^0.25.7",
    "typescript": "^5.3.3",
    "wasm-pack": "^0.12.1"
  },
  "peerDependencies": {
    "web-serial": "^1.0.0",
    "web-bluetooth": "^1.0.0"
  },
  "peerDependenciesMeta": {
    "web-serial": {
      "optional": true
    },
    "web-bluetooth": {
      "optional": true
    }
  },
  "engines": {
    "node": ">=16.0.0"
  },
  "publishConfig": {
    "access": "public"
  }
}